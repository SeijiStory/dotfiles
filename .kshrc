# If not running interactively, don"t do anything
source /etc/profile.d/plan9.sh # Plan9 Port
PS1=""
PS1+='$(pwd -L | sed "s,^$HOME,~,") $ '

# Aliases
alias cc=$CCOMPILER
alias clisp='echo "$0: clisp: not found; did you mean (((clisp)))?"'
alias connman="connmanctl"
alias dff='df -h | grep "^(\/dev|Filesystem)"'
alias dotfiles="git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME"
alias emacs="/bin/emacs -nw"
alias fastboot='/opt/android-sdk/platform-tools/fastboot'
alias fucking="sudo"
alias ls="ls -XL --file-type --color=never --group-directories-first"
alias mitscheme="rlwrap -r -c -f ~/.scheme_completion mitscheme"
alias mkcd=". mkcd.sh"
alias mkdir="mkdir -p"
alias oldvim="/bin/vim"
alias password="passwords.sh"
alias racket="rlwrap -r -c -f ~/.scheme_completion racket"
alias scheme="rlwrap -r -c -f ~/.scheme_completion $SCHEME"
alias tm="transmission-remote-cli"
alias transmission="transmission-remote-cli"
alias vim="nvim"
alias yt="rlwrap straw-viewer"
alias ytdl="youtube-dl -f 'bestvideo[ext=mp4]+bestaudio[ext=m4a]/mp4'"
alias ytmu="youtube-dl -x --audio-format mp3"

